double (a: int): int {
    v0: int = const 2;
    v1: int = mul a v0;
    ret v1;
}

triple (a: int): int {
    v0: int = const 3;
    v1: int = mul a v0;
    ret v1;
}

quad (a: int): int {
    v0: int = call double a;
    v1: int = call double a;
    v2: int = add v0 v1;
    ret v2;
}

oct (a: int): int {
    v0: int = call quad a;
    v1: int = call quad a;
    v2: int = add v0 v1;
    ret v2; 
}